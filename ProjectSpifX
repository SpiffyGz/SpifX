	end)
end
coroutine.wrap(WWZABTA_fake_script)()
local function NECW_fake_script() -- ScreenGui.LocalScript 
	local script = Instance.new('LocalScript', ScreenGui)

	local MainFrame = script.Parent.MainFrame
	local Open = false
	
	local PositionClosed = UDim2.new(0.346, 0,0.999, 0)
	local PositionOpen = UDim2.new(0.306, 0,0.144, 0)
	
	local UserInputService = game:GetService("UserInputService")
	
	UserInputService.InputBegan:Connect(function(KeyCode)
		if KeyCode.KeyCode == Enum.KeyCode.RightAlt then
			if Open then
				MainFrame:TweenPosition((PositionClosed), "Out", "Sine")
				Open = false
			else
				Open = true
				MainFrame:TweenPosition((PositionOpen), "Out", "Sine")
			end
		end
	end)
end
coroutine.wrap(NECW_fake_script)()
local function WSDPXC_fake_script() -- LOAD.LocalScript 
	local script = Instance.new('LocalScript', LOAD)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.LOAD.Visible = false
		script.Parent.Parent.LOADING.Visible = true
		wait(5)
		script.Parent.Parent.LOADING.Visible = false
		script.Parent.Parent.LOADED.Visible = true
		wait(1)
		script.Parent.Parent.Parent.MainFrame.Visible = true
		script.Parent.Parent.Parent.Frame.Visible = false
		game:GetService("StarterGui"):SetCore("SendNotification", {
			Title = "Welcome";
			Text = "Project SpifX Loaded";
			Button1 = "Close";
			Duration = 999;
		})
	end)
end
coroutine.wrap(WSDPXC_fake_script)()
