-- Scripts (all done by ya boy Spiffy):

local function DETCC_fake_script() -- HoodModded.LocalScript 
	local script = Instance.new('LocalScript', HoodModded)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Parent.HoodModdedFrame.Visible = true
		script.Parent.Parent.Parent.PhantomForcesFrame.Visible = false
		script.Parent.Parent.Parent.CounterBloxFrame.Visible = false
	end)
end
coroutine.wrap(DETCC_fake_script)()
local function DVSE_fake_script() -- PhantomForces.LocalScript 
	local script = Instance.new('LocalScript', PhantomForces)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Parent.HoodModdedFrame.Visible = false
		script.Parent.Parent.Parent.PhantomForcesFrame.Visible = true
		script.Parent.Parent.Parent.CounterBloxFrame.Visible = false
	end)
end
coroutine.wrap(DVSE_fake_script)()
local function JYTCZ_fake_script() -- CounterBlox.LocalScript 
	local script = Instance.new('LocalScript', CounterBlox)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Parent.HoodModdedFrame.Visible = false
		script.Parent.Parent.Parent.PhantomForcesFrame.Visible = false
		script.Parent.Parent.Parent.CounterBloxFrame.Visible = true
	end)
end
coroutine.wrap(JYTCZ_fake_script)()
local function YVZVIEE_fake_script() -- SpifXLock.LocalScript 
	local script = Instance.new('LocalScript', SpifXLock)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.UnlockLock.Visible = true
		wait(5)
		script.Parent.Parent.UnlockLock.Visible = false
	end)
end
coroutine.wrap(YVZVIEE_fake_script)()
local function WCEHM_fake_script() -- CreditsFrame.LocalScript 
	local script = Instance.new('LocalScript', CreditsFrame)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Parent.SpifX.Visible = false
		script.Parent.Parent.Parent.CreditsFrame.Visible = true
	end)
end
coroutine.wrap(WCEHM_fake_script)()
local function SGLN_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Parent.SpifX.Visible = true
		script.Parent.Parent.Parent.CreditsFrame.Visible = false
	end)
end
coroutine.wrap(SGLN_fake_script)()
local function HVDNQL_fake_script() -- chariotsware.LocalScript 
	local script = Instance.new('LocalScript', chariotsware)

	script.Parent.MouseButton1Click:Connect(function()
		loadstring(game:HttpGetAsync("https://raw.githubusercontent.com/Rippeed/DaHoodinary/main/chariotsware"))()
	end)
end
coroutine.wrap(HVDNQL_fake_script)()
